alias gcp='git commit -vp'
alias gff='git diff --ignore-blank-lines'
alias gp!='git push -f'
alias gl!='git fetch && git reset --hard origin/$(current_branch)'
alias grb='git rebase'
alias grbd='git rebase develop'
alias gup!='git pull --rebase; git submodule sync; git submodule update'
alias gsm='git submodule'
alias gsmup='git submodule update --init --recursive'
alias gsmup!='git submodule update --init --remote'
